<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Custom_ApiProducts::ApiProducts" title="ApiProducts" sortOrder="10">
                    <resource id="Custom_ApiProducts::ApiProducts_save" title="Save ApiProducts" sortOrder="10"/>
                    <resource id="Custom_ApiProducts::ApiProducts_delete" title="Delete ApiProducts" sortOrder="20"/>
                    <resource id="Custom_ApiProducts::ApiProducts_update" title="Update ApiProducts" sortOrder="30"/>
                    <resource id="Custom_ApiProducts::ApiProducts_view" title="View ApiProducts" sortOrder="40"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
